{
  "Version": "2012-10-17",
  "Id": "PolicyForCloudFrontPrivateContent",
  "Statement": [
    {
      "Sid": "CloudFront Access",
      "Effect": "Allow",
      "Principal": {
        "AWS": "${cloudfront_oai_arn}"
      },
      "Action": [
        "s3:GetObject"
      ],
      "Resource": [
        "${bucket_arn}/*"
      ]
    }
  ]
}
